{
  "name": "jebamo",
  "version": "0.0.19",
  "description": "Jebamo web component library",
  "repository": {
    "type": "git",
    "url": "https://github.com/pgreen00/jebamo"
  },
  "keywords": [
    "web",
    "component",
    "library",
    "typescript",
    "stencil",
    "jebamo"
  ],
  "author": "Peter Green",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "default": "./dist/loader.js"
    },
    "./je-*": {
      "types": "./dist/je-*.d.ts",
      "default": "./dist/je-*.js"
    }
  },
  "files": [
    "dist",
    "styles"
  ],
  "scripts": {
    "start": "concurrently --kill-others \"npm run watch:stencil\" \"npm run watch:eleventy\" \"npm run serve\"",
    "build": "stencil build && eleventy",
    "test": "playwright test",
    "generate": "stencil generate",
    "serve": "live-server www --port=3000 --no-browser",
    "watch:eleventy": "chokidar 'dist' 'src/components/**/*.md' 'docs' -c 'eleventy'",
    "watch:stencil": "stencil build --watch"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.12",
    "date-fns": "^4.1.0",
    "interactjs": "^1.10.27",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@playwright/test": "^1.55.1",
    "@stencil/core": "^4.7.0",
    "@types/node": "^24.5.2",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "glob": "^13.0.2",
    "live-server": "^1.2.2",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-multimd-table": "^4.2.3",
    "markdown-it-table-of-contents": "^1.1.0",
    "sass": "^1.90.0",
    "serve": "^14.2.5"
  },
  "license": "MIT"
}
