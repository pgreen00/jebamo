<!doctype html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <link rel="icon" type="image/svg+xml" href="/hex-tag.svg" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Jebamo</title>
  <link rel="stylesheet" href="/build/jebamo/styles/core.css" />
  <link rel="stylesheet" href="/build/jebamo/styles/classes.css" />
  <script type="module" src="/build/jebamo/dist/esm/jebamo.js"></script>
  <style>
  @import url("https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200");
  @import url("https://fonts.googleapis.com/css2?family=Work+Sans:ital,wght@0,100..900;1,100..900&display=swap");

  :root {
    font-family: "Work Sans";
    font-size: 16px;
    line-height: 1.75;
    font-weight: var(--je-font-regular);
  }

  body {
    color-scheme: light dark;
  }

  body.light {
    color-scheme: light;
  }

  body.dark {
    color-scheme: dark;
  }

  html,
  body {
    height: 100vh;
    width: 100vw;
    overflow: hidden;
    margin: 0;
    padding: 0;
  }

  h1,
  h2,
  h3,
  h4,
  h5,
  h6,
  p {
    margin-block-start: 0;
    margin-block-end: 0;
    margin-inline-start: 0;
    margin-inline-end: 0;
  }

  h1 {
    font-size: 2.5rem;
    font-weight: var(--je-font-bold);
  }

  h2 {
    font-size: 2rem;
    font-weight: var(--je-font-bold);
  }

  h3 {
    font-size: 1.75rem;
    font-weight: var(--je-font-bold);
  }

  h4 {
    font-size: 1.5rem;
    font-weight: var(--je-font-bold);
  }

  h5 {
    font-size: 1.25rem;
    font-weight: var(--je-font-bold);
  }

  h6 {
    font-size: 1rem;
    font-weight: var(--je-font-bold);
  }

  p {
    font-size: 1rem;
    font-weight: var(--je-font-regular);
  }

  small {
    font-size: 0.875rem;
    font-weight: var(--je-font-light);
  }
  </style>
</head>

<body class="dark">
  <je-page>
    <header>
      <je-toolbar>
        <img src="/hex-tag.svg" alt="logo" height="60" width="60" />
        <je-link href="/">
          <h1>Jebamo</h1>
        </je-link>
        <je-link href="https://github.com/pgreen00/jebamo" target="_blank" slot="end">
          <svg height="32" aria-hidden="true" viewBox="0 0 24 24" version="1.1" width="32" fill="currentcolor" stroke="currentcolor">
            <path d="M12 1C5.923 1 1 5.923 1 12c0 4.867 3.149 8.979 7.521 10.436.55.096.756-.233.756-.522 0-.262-.013-1.128-.013-2.049-2.764.509-3.479-.674-3.699-1.292-.124-.317-.66-1.293-1.127-1.554-.385-.207-.936-.715-.014-.729.866-.014 1.485.797 1.691 1.128.99 1.663 2.571 1.196 3.204.907.096-.715.385-1.196.701-1.471-2.448-.275-5.005-1.224-5.005-5.432 0-1.196.426-2.186 1.128-2.956-.111-.275-.496-1.402.11-2.915 0 0 .921-.288 3.024 1.128a10.193 10.193 0 0 1 2.75-.371c.936 0 1.871.123 2.75.371 2.104-1.43 3.025-1.128 3.025-1.128.605 1.513.221 2.64.111 2.915.701.77 1.127 1.747 1.127 2.956 0 4.222-2.571 5.157-5.019 5.432.399.344.743 1.004.743 2.035 0 1.471-.014 2.654-.014 3.025 0 .289.206.632.756.522C19.851 20.979 23 16.854 23 12c0-6.077-4.922-11-11-11Z"></path>
          </svg>
        </je-link>
        <je-button slot="end" onclick="document.body.classList.toggle('dark'); document.body.classList.toggle('light')">
          <je-icon fill>dark_mode</je-icon>
        </je-button>
      </je-toolbar>
    </header>
    <aside class="je-padding-horizontal-xs je-padding-vertical" style="width: 300px;">
      <nav>
				<je-tree>
          <je-branch label="Components" open>
            {% for item in collections.navigation | navToArray %}
              <je-branch label="{{ item.title }}" href="{{ item.url }}"></je-branch>
            {% endfor %}
          </je-branch>
          <je-branch label="Tutorials"></je-branch>
          <je-branch label="Changelog"></je-branch>
				</je-tree>
			</nav>
    </aside>
    <main>
      {% block content %}
        {{ content | safe }}
      {% endblock %}
    </main>
  </je-page>
</body>

</html>
